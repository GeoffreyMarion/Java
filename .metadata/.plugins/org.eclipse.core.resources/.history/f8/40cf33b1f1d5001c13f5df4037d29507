package nope;

public class semaine {
	
	public static void main(String[] args) {
		String[] semaine = {"lundi", "mar", "mercredi", "jeudi", "vendredi", "samedi", "dimanc"};
		for(int i=0;i<7;i++) {
			if(semaine[i] == "mar"){
			semaine[i] = "mardi";
			System.out.println(semaine[i]);
			}
		
		}
		for(int i=0;i<7;i++) {
			if(semaine[i] == "dimanc"){
			System.out.println(semaine.toString());
			}
		
		}
		for(int i=0;i<7;i++) {
			if(semaine[i] == "dimanc"){
				
			}
		
		}
	}
}