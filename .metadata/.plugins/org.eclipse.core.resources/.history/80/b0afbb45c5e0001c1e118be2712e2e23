package Vehicule;

public class Vehicule {
	protected int Roues;
	protected String Carburant;
	protected int Vitesse_max;

	protected int Nbr_roues() {
		return 0;
	}
	protected String T_carb() {
		return "Diesel";
	}
	protected int V_max() {
		return 500;
	}
	
	public Vehicule(int roues, String carburant, int vitesse_max) {
		super();
		Roues = roues;
		Carburant = carburant;
		Vitesse_max = vitesse_max;
	}
	
}

