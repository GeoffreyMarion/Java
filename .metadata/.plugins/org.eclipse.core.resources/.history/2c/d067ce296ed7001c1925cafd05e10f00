package Exo2;

public class enseignant {
	private int id;
	private Specialite specialite;
	
	public enseignant(Specialite specialite) {
		super();
		this.specialite = specialite;
	}

	public Specialite getSpecialite() {
		return specialite;
	}

	@Override
	public String toString() {
		return "[specialite=" + specialite + "]";
	}
}
