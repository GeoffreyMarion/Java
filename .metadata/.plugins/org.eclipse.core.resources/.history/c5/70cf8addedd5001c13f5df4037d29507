package nope;

public class Année {
	
	public static void main(String[] args) {
		int year=1999;
		if((year%4==0 && year%100!=0)||(year%400==0)){
			System.out.println("L'année "+ year +
					" est bisextile");
		}
		else {
			System.out.println("L'année "+ year +
					" est non bisextile");
		}
		
	}
	
}